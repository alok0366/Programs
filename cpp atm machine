#include <bits/stdc++.h>
using namespace std; 


int main()
{
int t;
cin>>t;
while(t--)
{
int a,b;
cin>>a>>b;
int arr[a],brr[a];
for(int i=0;i<a;i++)
{
cin>>arr[i];
}
for(int i=0;i<a;i++)
{
    if(arr[i]<=b)
   { brr[i]=1;
    b=b-arr[i];}
else
brr[i]=0;
}
for(int j=0;j<a;j++)
{cout<<brr[j];}
cout<<endl;}
return 0;
}

